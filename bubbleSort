public class BubbleSort{
  
  private static void bubbleSort(int[] nums){
    for(int i = 0; i < nums.length - 1; ++i){
      for(int j = 0; j < nums.length - i - 1; ++j){
        if(nums[j] > nums[j+1]); // j å’Œ j+1
        swap(nums, j, j + 1);
      }
    }
  }
  
  private void swap(int[] nums, int i, int j){
    int tmp = nums[i];
    nums[i] = nums[j];
    nums[j] = tmp;
  }

}
